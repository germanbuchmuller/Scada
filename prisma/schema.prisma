// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Register {
  id        Int      @id @default(autoincrement())
  status    Status
  type      Type?
  createdAt DateTime @default(now())
}

enum Status {
  IDLE
  READING
}

enum Type {
  LOW_METAL
  HIGH_METAL
  LOW_NO_METAL
  HIGH_NO_METAL
}
